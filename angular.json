{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "where": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/where",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets/custom",
              "src/assets/vendor"

            ],
            "styles": [
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/vendor/animate.css-master/animate.min.css",
              "src/assets/vendor/loadscreen/css/spinkit.css",
              "src/assets/vendor/fontawesome/css/fontawesome-all.min.css",
              "src/assets/custom/css/fables-icons.css",
              "src/assets/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/vendor/bootstrap/css/bootstrap-4-navbar.css",
              "src/assets/vendor/portfolio-filter-gallery/portfolio-filter-gallery.css",
              "src/assets/vendor/fancybox-master/jquery.fancybox.min.css",
              "src/assets/vendor/range-slider/range-slider.css",
              "src/assets/vendor/owlcarousel/owl.carousel.min.css",
              "src/assets/vendor/owlcarousel/owl.theme.default.min.css",
              "src/assets/custom/css/custom.css",
              "src/assets/custom/css/custom-responsive.css",
              "src/assets/vendor/bootstrap/css/bootstrap-4-navbar.css",
              "src/assets/vendor/bootstrap/css/bootstrap.min.css"
           
            ],
            "scripts": ["node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
          "src/assets/vendor/bootstrap/js/bootstrap-4-navbar.js",
          "src/assets/custom/js/custom.js",
          "src/assets/vendor/jquery/jquery-3.3.1.min.js",
"src/assets/vendor/loadscreen/js/ju-loading-screen.js",
"src/assets/vendor/jquery-circle-progress/circle-progress.min.js",
"src/assets/vendor/popper/popper.min.js",
"src/assets/vendor/jQuery.countdown-master/jquery.countdown.min.js",
"src/assets/vendor/timeline/jquery.timelify.js",
"src/assets/vendor/bootstrap/js/bootstrap.min.js",
"src/assets/vendor/bootstrap/js/bootstrap-4-navbar.js",
"src/assets/vendor/WOW-master/dist/wow.min.js",
"src/assets/vendor/owlcarousel/owl.carousel.min.js",
"src/assets/custom/js/jquery-data-to.js",
"src/assets/vendor/jquery-circle-progress/circle.js",
"src/assets/vendor/portfolio-filter-gallery/jquery.isotope.min.js",
"src/assets/vendor/portfolio-filter-gallery/portfolio-filter-gallery.js",
"/src/assets/vendor/fancybox-master/jquery.fancybox.min.js"


      
      ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "where:build:production"
            },
            "development": {
              "buildTarget": "where:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "where:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
